@page "/food"


<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Amount</th>
            <th>Price</th>
            <th>Imported From</th>
            <th>Imported Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var food in Foods)
        {
        <td>@food.FoodName</td>
        <td>@food.Amount</td>
        <td>@food.Price</td>
        <td>@food.ImportedFrom</td>
        <td>@food.CreatedDate</td>
        }
    </tbody>
</table>

